name: server
services:
  codeserver:
    container_name: codeserver
    environment:
      PASSWORD: 
      PGID: 1000
      PUID: 1000
      SUDO_PASSWORD: 
      TZ: {{ tz }}
    hostname: DockSTARTer
    image: lscr.io/linuxserver/code-server
    logging:
      driver: json-file
      options:
        max-file: 10
        max-size: 200k
    networks:
      default: null
    ports:
    - mode: ingress
      target: 8443
      published: 8443
      protocol: tcp
    restart: unless-stopped>
    - type: bind
      source: /etc/localtime
      target: /etc/localtime
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: ~/.config/appdata/codeserver
      target: /config
      bind:
        create_host_path: true
    - type: bind
      source: ~/storage
      target: /storage
      bind:
        create_host_path: true
  gitea:
    container_name: gitea
    environment:
      TZ: {{ tz }}
      USER_GID: 1000
      USER_UID: 1000
    hostname: DockSTARTer
